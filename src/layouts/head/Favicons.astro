---
const BASE_URL = import.meta.env.BASE_URL;
---

<link rel="icon" type="image/x-icon" href={`${BASE_URL}content/images/favicons/favicon.ico`} />
<link rel="icon" type="image/png" sizes="16x16" href={`${BASE_URL}content/images/favicons/favicon-16x16.png`} />
<link rel="icon" type="image/png" sizes="32x32" href={`${BASE_URL}content/images/favicons/favicon-32x32.png`} />
<link rel="apple-touch-icon" sizes="180x180" href={`${BASE_URL}content/images/favicons/apple-icon-180x180.png`} />
<link
    rel="icon"
    type="image/png"
    sizes="192x192"
    href={`${BASE_URL}content/images/favicons/android-icon-192x192.png`}
/>
<link rel="manifest" href={`${BASE_URL}content/images/favicons/manifest.json`} />
