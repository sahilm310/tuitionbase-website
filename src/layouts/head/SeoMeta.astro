---
import { websiteUrl } from "../../config/constants";
---

<meta
    name="description"
    content="Professional tuition centres across London teaching KS1 to A Level Maths, English, Biology, Chemistry and Physics with affordable pricing"
/>
<meta name="title" content="TuitionBase" />
<meta
    name="Keywords"
    content="tuition, tutors, tutors in london, english tuition, maths tuition, science tuition, tuition in london, tuition centre, gcse, london"
/>

<meta name="revisit-after" content="3 days" />
<meta name="Reply-to" content="info@tuitionbase.co.uk" />
<meta name="coverage" content="worldwide" />
<meta name="language" content="en-GB" />
<meta name="Classification" content="Tuition classes for A-levels, GCSEs, KS3, KS2 and KS1 with affordable pricing" />
<meta name="Author" content="tuitionbase.co.uk" />

<!-- Facebook Meta Tags -->
<meta property="og:url" content="https://tuitionbase.co.uk/" />
<meta property="og:type" content="website" />
<meta property="og:title" content="TuitionBase" />
<meta property="og:description" content="Offering expert and personalised tuition at affordable prices." />
<meta property="og:image" content={`${websiteUrl}/content/images/meta/og_image_630.png`} />
<meta property="og:locale" content="en_GB" />
<meta property="og:site_name" content="TuitionBase" />
<meta property="article:author" content="TuitionBase" />
<meta property="article:publisher" content="TuitionBase" />

<!-- Twitter Meta Tags -->
<meta name="twitter:card" content="summary_large_image" />
<meta property="twitter:domain" content="tuitionbase.co.uk" />
<meta property="twitter:url" content="https://tuitionbase.co.uk/" />
<meta name="twitter:title" content="TuitionBase" />
<meta name="twitter:description" content="Offering expert and personalised tuition at affordable prices." />
<meta name="twitter:image" content={`${websiteUrl}/content/images/meta/og_image_630.png`} />

<meta name="robots" content="index, follow" />

<meta name="geo.region" content="GB-LND" />
<meta name="geo.placename" content="London" />
<meta name="geo.position" content="51.629444;-0.055889" />
<meta name="ICBM" content="51.629444, -0.055889" />
<meta name="DC.title" content="TuitionBase - London Tuition Centre" />

<!-- Additional Meta Tags -->
<meta name="subject" content="Educational Services" />
<meta name="topic" content="Private Tutoring London" />
<meta
    name="summary"
    content="Expert tutoring services across London for all key stages with small group classes and affordable pricing"
/>
<meta name="category" content="Education" />
<meta name="target" content="students, parents, education" />
<meta name="audience" content="students, parents, educators" />
<meta name="distribution" content="global" />
<meta name="rating" content="general" />
<meta name="HandheldFriendly" content="True" />
<meta name="MobileOptimized" content="320" />

<!-- Language and Canonical Links -->
<link rel="canonical" href={`${websiteUrl}`} />
<link rel="alternate" hreflang="x-default" href={`${websiteUrl}`} />
